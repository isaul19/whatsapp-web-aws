{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": false
    },
    "transform": null,
    "target": "es2016",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false,
    "baseUrl": "./",

    "paths": {
      "@boostrap/*": ["./src/boostrap/*"],
      "@adapters/*": ["./src/config/adapters/*"],
      "@routers/*": ["./src/routers/*"],
      "@controllers/*": ["./src/controllers/*"],
      "@services/*": ["./src/services/*"]
    }
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "noInterop": false,
    "lazy": false
  },
  "minify": true
}
